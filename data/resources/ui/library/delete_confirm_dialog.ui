<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <template class="DeleteConfirmDialog" parent="AdwAlertDialog">
    <property name="heading" translatable="yes">Delete permanently?</property>
    <property name="body" translatable="yes">"{filename}" will be permanently deleted.</property>
    <property name="default-response">delete</property>
    <property name="close-response">cancel</property>
    <responses>
      <response id="cancel" translatable="yes">_Cancel</response>
      <response id="delete" translatable="yes" appearance="destructive">_Delete</response>
    </responses>
  </template>
</interface>