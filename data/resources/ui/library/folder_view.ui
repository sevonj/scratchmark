<?xml version="1.0" encoding="UTF-8"?>
<interface>
    <template class="FolderView" parent="AdwBin">
        <child>
            <object class="GtkBox">
                <property name="orientation">GTK_ORIENTATION_VERTICAL</property>
                <child>
                    <object class="AdwBin" id="expand_button_cont">
                        <child>
                            <object class="GtkToggleButton" id="expand_button">
                                <style>
                                    <class name="flat" />
                                </style>
                                <child>
                                    <object class="GtkBox" id="title_row">
                                        <property name="orientation">GTK_ORIENTATION_HORIZONTAL</property>
                                        <property name="spacing">4</property>
                                        <child>
                                            <object class="GtkImage" id="expand_icon">
                                                <style>
                                                    <class name="dim-label" />
                                                </style>
                                                <property name="icon-name">down-small-symbolic</property>
                                                <property name="icon-size">GTK_ICON_SIZE_NORMAL</property>
                                            </object>
                                        </child>
                                        <child>
                                            <object class="GtkImage" id="folder_icon">
                                                <style>
                                                    <class name="dim-label" />
                                                </style>
                                                <property name="icon-name">folder-symbolic</property>
                                                <property name="icon-size">GTK_ICON_SIZE_NORMAL</property>
                                            </object>
                                        </child>
                                        <child>
                                            <object class="GtkLabel" id="title">
                                                <property name="label">dir-name-here</property>
                                                <property name="xalign">0</property>
                                                <property name="ellipsize">PANGO_ELLIPSIZE_END</property>
                                                <attributes>
                                                    <attribute name="weight"
                                                        value="PANGO_WEIGHT_NORMAL" />
                                                </attributes>
                                            </object>
                                        </child>
                                    </object>
                                </child>
                            </object>
                        </child>
                    </object>
                </child>
                <child>
                    <object class="GtkBox" id="content_vbox">
                        <property name="orientation">GTK_ORIENTATION_VERTICAL</property>
                        <property name="spacing">2</property>
                        <child>
                            <object class="GtkBox" id="subdirs_vbox">
                                <property name="orientation">GTK_ORIENTATION_VERTICAL</property>
                                <property name="spacing">2</property>
                            </object>
                        </child>
                        <child>
                            <object class="GtkBox" id="documents_vbox">
                                <property name="orientation">GTK_ORIENTATION_VERTICAL</property>
                                <property name="spacing">2</property>
                            </object>
                        </child>
                    </object>
                </child>
            </object>
        </child>
    </template>
</interface>